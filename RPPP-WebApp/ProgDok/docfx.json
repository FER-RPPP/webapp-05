{
  "metadata": [
    {
      "src": [
        {
          "src": "../RPPP-WebApp",
          "files": [
            "*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "exclude": [
          "_site/**"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "RPPP-WebApp",
      "_appTitle": "RPPP-WebApp",
      "_appFooter": "&copy; FER,RPPP05,2024",
      "_enableSearch": true,
      "pdf": false
    }
  }
}